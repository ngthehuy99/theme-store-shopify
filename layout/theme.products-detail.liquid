<script>
  //click open menu

  		var menu = document.getElementById('myheader');
        var btnMenu = document.getElementById('navbar-btn');
		var sub_content_menu = document.getElementById("Menu");
        var menuHeight = menu.clientHeight;
        btnMenu.onclick = function(){
        var isClose = menu.clientHeight=== menuHeight;
            if(isClose){
                menu.style.height = 'auto';
              	sub_content_menu.classList.add("active-content-menu");
                    
            }
            else{
                menu.style.height = null;
      			sub_content_menu.classList.remove("active-content-menu");

            }
        }

  
  //     Scroll back
  var mybutton = document.getElementById("myBtn");

  window.onscroll = function() {scrollFunction();
                               myFunction()};

  function scrollFunction() {
    if (document.body.scrollTop > 500 || document.documentElement.scrollTop > 500) {
      mybutton.style.display = "block";
    } else {
      mybutton.style.display = "none";
    }
  }
  function topFunction() {
    document.body.scrollTop = 0;
    document.documentElement.scrollTop = 0;
  }

//   sticky
  var header = document.getElementById("myheader");
  var sticky = header.offsetTop;
  function myFunction() {
    if (window.pageYOffset > sticky) {
      header.classList.add("sticky");
       
      
    } else {
      header.classList.remove("sticky");
    }
  }

  
  

  $('.owl-carousel').owlCarousel({
    loop:false,
    margin:15,
    nav:true,
    navText:["<i class='btn-nav fas fa-angle-left'></i>","<i class='btn-nav fas fa-angle-right'></i>"],
    dots:false,
    responsive:{

      0:{

        items:1
      },
      600:{

        items:2
      },
      1000:{

        items:3
      }
    }
  })


</script>

